{
    "name": "digitalie/laravel-uuid",
    "description": "Laravel UUID Package",
    "type": "library",
    "require": {
        "php": "^7.0",
        "ramsey/uuid": "^3.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "LaravelUuid\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelUuid\\UuidServiceProvider"
            ]
        }
    },
    "keywords": [
        "UUID",
        "Laravel"
    ],
    "authors": [
        {
            "name": "Shmavon Gazanchyan",
            "email": "sgazanchyan@gmail.com"
        }
    ],
    "homepage": "https://github.com/MunGell/laravel-uuid",
    "support": {
        "source": "https://github.com/MunGell/laravel-uuid",
        "issues": "https://github.com/MunGell/laravel-uuid"
    },
    "license": "MIT"
}
